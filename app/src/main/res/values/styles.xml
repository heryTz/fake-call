<resources>
    <style name="Widget.Theme.FakeCall.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>

    <style name="Widget.App.ButtonNumber" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_columnWeight">1</item>
        <item name="android:textSize">24dp</item>
        <item name="cornerRadius">16dp</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">#d5d5d5</item>
    </style>

    <style name="Background">
        <item name="android:background">#F6F6F6</item>
    </style>
</resources>